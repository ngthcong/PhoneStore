
@{
    Layout = null;
}



<!DOCTYPE html>
<html lang="en">
<head>
    <title>Đăng nhập</title>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!--===============================================================================================-->
    <link rel="icon" type="image/png" href="~/dist/img/login/icons/favicon.ico" />
    <script type="text/javascript">
        function preventBack() { window.history.forward(); }
        setTimeout("preventBack()", 0);
        window.onunload = function () { null };
    </script>
    <!--===============================================================================================-->

    <link href="~/dist/css/bootstrap/css/bootstrap.css" rel="stylesheet" />
    <!--===============================================================================================-->

    <link href="~/fonts/font-awesome-4.7.0/css/font-awesome.css" rel="stylesheet" />
    <!--===============================================================================================-->

    <link href="~/fonts/iconic/css/material-design-iconic-font.css" rel="stylesheet" />

    <link rel="stylesheet" type="text/css" href="~/styles/util.css">

    <link rel="stylesheet" type="text/css" href="~/styles/main.css">
    <!--===============================================================================================-->
</head>
<body>

    <div class="limiter">
        <div class="container-login100" style="background-image: url('@Url.Content("~/dist/img/login/bg-01.jpg")');">
            <div class="wrap-login100 p-l-55 p-r-55 p-t-65 p-b-54">
                <form id="loginUser" class="login100-form validate-form">
                    <span class="login100-form-title p-b-49">
                        Đăng nhập
                    </span>

                    <div class="wrap-input100 validate-input m-b-23">
                        <div class="input-group2">
                            <span class="label-input100">Email</span>
                            <input class="input100" id="Email" type="text" name="Email">
                            <span class="focus-input100" data-symbol="&#xf206;"></span>
                        </div>
                        
                    </div>

                    <div class="wrap-input100 validate-input">
                        <div class="input-group2">
                            <span class="label-input100">Mật khẩu</span>
                            <input class="input100" id="Pass" type="password" name="Pass">
                            <span class="focus-input100" data-symbol="&#xf190;"></span>
                        </div>
                        
                    </div>

                    <div class="text-right p-t-8 p-b-31">
                        <a href="#">
                            Quên mật khẩu?
                        </a>
                    </div>

                    <div class="flex-col-c">
                        <div class="mx-auto" id="error">

                        </div>
                    </div>

                    <div class="container-login100-form-btn">
                        <div class="wrap-login100-form-btn">
                            <div class="login100-form-bgbtn"></div>
                            <button type="submit" class="login100-form-btn">
                                Đăng nhập
                            </button>
                        </div>
                    </div>
                    <div class="flex-col-c p-t-155">
                        <span class="txt1 p-b-17">
                            Hoặc đăng ký tài khoản
                        </span>

                        <a href="#" class="txt2">
                            Đăng ký
                        </a>
                    </div>
                </form>
            </div>
        </div>
    </div>


    <!--===============================================================================================-->

    <script src="~/plugins/jquery/jquery.min.js"></script>
    <script src="~/plugins/bootstrap/popper.js"></script>

    <script src="~/plugins/bootstrap/js/bootstrap.js"></script>
    <script src="~/plugins/bootstrap/popper.js"></script>
    <script src="~/plugins/jquery-validation/jquery.validate.js"></script>
    <script src="~/js/loginuser.js"></script>

</body>
</html>